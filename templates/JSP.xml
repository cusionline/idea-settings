<templateSet group="JSP">
  <template name="dol" value="${$THIS$}" description="Element selection (dollar + brackets)" toReformat="false" toShortenFQNames="true">
    <variable name="THIS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="cif" value="&lt;c:if test=&quot;${$COND$}&quot;&gt;$END$&lt;/c:if&gt;" description="jstl c:if" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="cset" value="&lt;c:set var=&quot;$VAR$&quot; value=&quot;${$VALUE$$END$}&quot;/&gt;" description="jstl c:set" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="cchoose" value="&lt;c:choose&gt;&#10;  &lt;c:when test=&quot;${$COND$}&quot;&gt;&#10;    $END$&#10;  &lt;/c:when&gt;&#10;  &lt;c:otherwise&gt;&#10;    &#10;  &lt;/c:otherwise&gt;&#10;&lt;/c:choose&gt;" description="jstl c:choose" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="cwhen" value="&lt;c:when test=&quot;${$COND$}&quot;&gt;&#10;  $END$&#10;&lt;/c:when&gt;" description="jstl c:when" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="cfor" value="&lt;c:forEach var=&quot;$VAR$&quot; items=&quot;${$ITEMS$}&quot;&gt;&#10;&lt;/c:forEach&gt;" description="jstl c:forEach" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ITEMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
  <template name="elm" value="${elx.msg('$THIS$')}" description="ElFunctionBean Message (elx.msg())" toReformat="false" toShortenFQNames="true">
    <variable name="THIS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="true" />
    </context>
  </template>
</templateSet>