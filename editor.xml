<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
    <option name="FONT_FAMILY" value="FiraCode+ Roboto Mono" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Roboto Mono" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_INLINE_DIALOG" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\boptimize\b.*" />
    <filter name="Optimize">
      <pattern index="2" />
    </filter>
  </component>
</application>